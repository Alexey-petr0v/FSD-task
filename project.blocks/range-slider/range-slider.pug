mixin addRangeSlider
    div(class="range-slider")
        div(class="range-slider__title")
            .range-slider__title-column
                h3 RANGE SLIDER
            .range-slider__title-column
                h3(class="range-slider__values")
                    <span id='range-slider-value1'>5 000</span>₽ - <span id='range-slider-value2'>10 000</span>₽
        div(id="target")
            div(id="mydiv1")
                div(id="mydiv1header")
            div(id="content")
            div(id="mydiv2")
                div(id="mydiv2header")
        div(id="other")= "Trigger the handler"
        div(id="log")