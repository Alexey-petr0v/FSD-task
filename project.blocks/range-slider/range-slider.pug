mixin addRangeSlider
    .range-slider
        .range-slider__title
            .range-slider__title-column
                h3 RANGE SLIDER
            .range-slider__title-column
                h3(class="range-slider__values")
                    <span id='range-slider-output1'>5 000</span>â‚½ - <span id='range-slider-output2'>10 000</span>â‚½
        .range-slider__slider(id="target")
            input(name='range-slider', data-output1='5 000', data-output2='10 000', value='{output1: "5 000", output2: "10 000"}', type='hidden')
            .range-slider__limit.range-slider__limit_left(class="mydiv1")
                div(class="mydiv1header")
            .range-slider__accent(id="content")
            .range-slider__limit.range-slider__limit_right(class="mydiv2")
                div(class="mydiv2header")